{
  "name": "task-management-system",
  "private": true,
  "version": "0.0.0",
  "description": "TMS Backend API",
  "scripts": {
    "build": "bash dev.sh && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:test": "dotenv -e .env.test -- nest start --watch",
    "start:dev": "bash dev.sh && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:check": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit:clear": "jest --config ./jest.config.js --clear-cache",
    "test:e2e:clear": "jest --config ./jest-e2e.json --clear-cache",
    "test:unit": "npm run test:unit:clear && jest --config ./jest.config.js --maxWorkers=50% --detectOpenHandles --forceExit",
    "test:unit:full": "npm run test:unit:first-suite && npm run test:unit:second-suite && npm run test:unit:third-suite && npm run test:unit:fourth-suite",
    "test:unit:first-suite": "npm run test:unit \"src/domains/(auth|authenticator)/.*\\.*\\.spec.ts$\"",
    "test:unit:second-suite": "npm run test:unit \"src/domains/(business|sessions)/.*\\.*\\.spec.ts$\"",
    "test:migration": "dotenv -e .env.test -- typeorm-ts-node-esm migration:run -d ormconfig-test",
    "test:migration-revert": "dotenv -e .env.test -- typeorm-ts-node-esm migration:revert -d ormconfig-test",
    "test:watch": "jest --config ./jest.config.js --watch",
    "test:cov": "jest --config ./jest.config.js --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --maxWorkers=50%",
    "test:e2e:all": "npm run test:e2e:full",
    "test:e2e:full": "jest --config ./jest-e2e.json --runInBand --clearMocks --detectOpenHandles --forceExit",
    "test:e2e:first-suite": "npm run test:e2e:all \"test/(auth|authenticator)/.*\\.controller\\.e2e-spec.ts$\"",
    "test:e2e:second-suite": "npm run test:e2e:all \"test/(business|sessions)/.*\\.controller\\.e2e-spec.ts$\"",
    "test:e2e": "npm run test:e2e:clear && npm run test:e2e:first-suite && npm run test:e2e:second-suite && npm run test:e2e:third-suite && npm run test:e2e:fourth-suite",
    "prepare": "husky install",
    "migration:generate": "typeorm-ts-node-esm migration:generate -d ormconfig ./src/database/mysql/migrations/${npm_config_name}",
    "migration:create": "typeorm-ts-node-esm migration:create ./src/database/mysql/migrations/${npm_config_name}",
    "migration:run": "typeorm-ts-node-esm migration:run -d ormconfig",
    "migration:revert": "typeorm-ts-node-esm migration:revert -d ormconfig",
    "sql:seed:config": "typeorm-seeding config -c ./seeding-source.js",
    "sql:seed:run": "typeorm-seeding seed -c ./seeding-source.js",
    "sql:seed:run-class": "typeorm-seeding seed --s ${npm_config_name} -c ./seeding-source.js",
    "jobs:create": "ts-node src/task-scheduler/commands/create-job.command.ts"
  },
  "dependencies": {
    "@aspecto/opentelemetry": "^0.0.139",
    "@aws-sdk/client-s3": "^3.312.0",
    "@aws-sdk/lib-storage": "^3.312.0",
    "@nestjs/axios": "^2.0.0",
    "@nestjs/bull": "^0.6.3",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/event-emitter": "^1.4.1",
    "@nestjs/jwt": "^10.0.1",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/schedule": "^2.2.0",
    "@nestjs/swagger": "^6.1.4",
    "@nestjs/throttler": "^3.1.0",
    "@nestjs/typeorm": "^9.0.1",
    "@types/bcrypt": "^5.0.0",
    "app-root-path": "^3.1.0",
    "aws-sdk": "^2.1354.0",
    "axios": "^1.3.2",
    "bcrypt": "^5.1.0",
    "better-sqlite3": "^8.4.0",
    "body-parser": "^1.20.1",
    "bull": "^4.10.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "cron": "^2.4.1",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^6.0.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "express-ctx": "^0.1.1",
    "google-auth-library": "^8.7.0",
    "helmet": "^6.0.1",
    "mailgun.js": "^8.0.6",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^2.3.3",
    "nestjs-real-ip": "^3.0.1",
    "nodemailer": "^6.9.1",
    "otplib": "^12.0.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "qrcode": "^1.5.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.5",
    "sqlite3": "^5.1.4",
    "typeorm": "^0.3.0",
    "uuid": "^9.0.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@concepta/typeorm-seeding": "^4.0.0-beta.0",
    "@faker-js/faker": "^7.6.0",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cron": "^2.4.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.8",
    "@types/json-schema": "^7.0.12",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.0.3",
    "@types/passport-jwt": "^3.0.8",
    "@types/qrcode": "^1.5.0",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "cross-env": "^7.0.3",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-sonarjs": "^0.17.0",
    "eslint-plugin-unicorn": "^45.0.2",
    "husky": "^8.0.0",
    "jest": "^28.1.2",
    "pm2": "^5.3.0",
    "prettier": "^2.7.1",
    "redis-memory-server": "^0.6.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.8",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  },
  "engines": {
    "npm": ">=9.2.0",
    "node": ">=18.13.0",
    "yarn": ">=1.22.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "setupFiles": [
      "../test/set-env-vars.ts"
    ],
    "modulePaths": [
      "<rootDir>"
    ]
  }
}
