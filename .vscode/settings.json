{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.organizeImports": "never",
    "source.removeUnusedImports": "explicit"
  },
  "cSpell.words": [
    "dconfiguration",
    "devs",
    "dtos",
    "longcode",
    "longtext",
    "multipage",
    "nestjs",
    "ormconfig",
    "otplib",
    "otps",
    "qrcode",
    "typeorm",
    "validatorjs"
  ]
}
